odo.checked } : todo\n    //   )\n    // );\n    dispatch({ type: \"TOGGLE\", id });\n  }, []);\n\n  return (\n    <TodoTemplate>\n      <TodoInsert onInsert={onInsert} />\n      <TodoList todos={todos} onRemove={onRemove} onToggle={onToggle} />\n    </TodoTemplate>\n  );\n}\n\nexport default App;\n","/Applications/Coding/FrontEnd/리액트를다루는기술/todo-app/src/Components/TodoTemplate.js",[],"/Applications/Coding/FrontEnd/리액트를다루는기술/todo-app/src/Components/TodoInsert.js",[],"/Applications/Coding/FrontEnd/리액트를다루는기술/todo-app/src/Components/TodoList.js",[],"/Applications/Coding/FrontEnd/리액트를다루는기술/todo-app/src/Components/TodoListItem.js",[],{"ruleId":"35","replacedBy":"36"},{"ruleId":"37","replacedBy":"38"},{"ruleId":"39","severity":1,"message":"40","line":1,"column":17,"nodeType":"41","messageId":"42","endLine":1,"endColumn":25},"no-native-reassign",["43"],"no-negated-in-lhs",["44"],"no-unused-vars","'useState' is defined but never used.","Identifier","unusedVar","no-global-assign","no-unsafe-negation"]